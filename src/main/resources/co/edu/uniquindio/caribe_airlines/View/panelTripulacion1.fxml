<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="panelTripulacion1" prefHeight="680.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.caribe_airlines.viewController.PanelTripulacionController1">
   <children>
      <Label alignment="CENTER" contentDisplay="TOP" prefHeight="35.0" prefWidth="900.0" style="-fx-background-color: Black;" text="Asignacion de Tripulacion" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="tablaTripulacion" layoutX="41.0" layoutY="50.0" prefHeight="500.0" prefWidth="800.0">
         <columns>
            <TableColumn fx:id="colAvion" text="Avion" />
            <TableColumn fx:id="colTripulante" text="Tripulante" />
            <TableColumn fx:id="colRango" text="Rango" />
         </columns>
      </TableView>
      <ListView fx:id="listaTripulantesDisponibles" layoutX="41.0" layoutY="570.0" prefHeight="100.0" prefWidth="300.0" />
      <ComboBox fx:id="comboAviones" layoutX="360.0" layoutY="570.0" prefWidth="150.0" />
      <Button fx:id="btnAsignar" layoutX="520.0" layoutY="570.0" mnemonicParsing="false" onAction="#asignarTripulante" text="Asignar" />
      <Button fx:id="btnEliminar" layoutX="520.0" layoutY="620.0" mnemonicParsing="false" onAction="#eliminarTripulante" text="Eliminar" />
      <Button fx:id="btnVolver" layoutX="41.0" layoutY="630.0" mnemonicParsing="false" onAction="#previousPanel" prefHeight="46.0" prefWidth="113.0" text="Volver" />
   </children>
</AnchorPane>